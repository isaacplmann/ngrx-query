{
  "name": "ngrx-query",
  "version": "0.1.2",
  "repository": {
    "type": "git",
    "url": "git://github.com/isaacplmann/ngrx-query.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:demo",
    "build:demo": "rimraf dist/demo && ng build",
    "build:lib": "rimraf dist/lib && ngc && copyfiles package.json README.md LICENSE dist/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pub": "npm run pub:lib && npm run pub:demo",
    "pub:demo": "npm run build:demo -- --base-href /ngrx-query/ && gh-pages -d dist/demo",
    "pub:lib": "npm run build:lib && npm publish dist",
    "pack:lib": "npm run build:lib && cd dist && npm pack && copyfiles *.tgz ../ && cd .. && rimraf dist/*.tgz"
  },
  "private": false,
  "peerDependencies": {
    "@angular/core": "^2.4.10",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.1",
    "@ngrx/store": "^2.2.1"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.2",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.2",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.4",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "gh-pages": "0.12.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "json-stable-stringify": "^1.0.1",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.17.4",
    "protractor": "~5.1.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.2.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.1",
    "zone.js": "^0.8.4"
  },
  "dependencies": {
    "redux-query": "^1.3.0"
  }
}
