{
  "name": "ngrx-query",
  "version": "2.1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/isaacplmann/ngrx-query.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:record": "ng serve -e=record",
    "start:mock": "ng serve -e=mock",
    "build": "npm run build:lib && npm run build:demo",
    "build:demo": "rimraf dist/demo && ng build",
    "build:lib": "rimraf dist/lib && ngc && copyfiles package.json README.md LICENSE dist/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "pub": "npm run pub:lib && npm run pub:demo",
    "pub:demo": "npm run build:demo -- --base-href /ngrx-query/ && gh-pages -d dist/demo",
    "pub:lib": "npm run build:lib && npm publish dist",
    "pack:lib": "npm run build:lib && cd dist && npm pack && copyfiles *.tgz ../ && cd .. && rimraf dist/*.tgz"
  },
  "private": false,
  "peerDependencies": {
    "@angular/core": "^4.3.0 || ^5.0.0",
    "@angular/common": "^4.3.0 || ^5.0.0",
    "@ngrx/effects": "^4.0.2",
    "@ngrx/store": "^4.0.2",
    "rxjs": "^5.5.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "^4.1.1",
    "@types/jasmine": "^2.6.3",
    "@types/node": "^8.0.50",
    "codelyzer": "^3.2.2",
    "copyfiles": "^1.2.0",
    "core-js": "^2.5.1",
    "cypress": "^1.0.3",
    "gh-pages": "0.12.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "json-stable-stringify": "^1.0.1",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.17.4",
    "protractor": "~5.1.0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "zone.js": "^0.8.18"
  },
  "dependencies": {
    "redux-query": "^2.1.0"
  }
}
